<% content_for :body_class, 'cieszyn' %>
<div class='container'>
  <%= render 'cieszyn/bar' %>
  <h2>Cennik</h2>
  <% if(@jobs) %>
    <h3>Ceny usług</h3>
    <table>
      <tr><th>Usługa</th><th>Cena w złotych</th></tr>
      <% @jobs.each do |job| %>
        <tr><td><%=job.name%></td><td><%=job.price%>,-</td></tr>
      <% end %>
    </table>
  <% end %>
</div>
